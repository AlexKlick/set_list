<div class="container-fluid">
  <h1>Songs</h1>
  <table class="table table-dark table-striped table-bordered">
    <tr>
      <th scope="col">title</th>
      <th scope="col">length</th>
      <th scope="col">play-count</th>
    </tr>
    <% @songs.each do |song| %>
    <tr>
      <td><%= song.title %></td>
      <td><%= song.length %></td>
      <td><%= song.play_count %></td>
    </tr>
    <% end %>
  </table>
</div>
